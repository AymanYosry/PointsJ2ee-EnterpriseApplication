<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets" xmlns:p="http://primefaces.org/ui" xmlns:em="http://xmlns.jcp.org/jsf/composite/emcomp">



<ui:composition template="/resources/templates/common/layout.xhtml">
	<ui:define name="search_panel">
		<p:panelGrid columns="6" styleClass="ui-noborder">

			<p:outputLabel for="paymentDateFrom" value="#{labels.from} #{labels.paymentDate}" />
			<p:calendar id="paymentDateFrom" value="#{agentAdminAgentPaymentBean.paymentDateFromSearch}" pattern="#{entityConstantsBean.dateFormat}" />
			<p:outputLabel for="paymentDateTo" value="#{labels.to} #{labels.paymentDate}" />
			<p:calendar id="paymentDateTo" value="#{agentAdminAgentPaymentBean.paymentDateToSearch}" pattern="#{entityConstantsBean.dateFormat}" />
			<p:commandButton value="#{labels.search}" id="search" update="growl search_table" actionListener="#{agentAdminAgentPaymentBean.getPayments()}"
				 />
		</p:panelGrid>

	</ui:define>
	<ui:define name="content_center">
		<em:search_component table_bean="#{agentAdminAgentPaymentBean}"></em:search_component>
	</ui:define>
</ui:composition>

</html>

