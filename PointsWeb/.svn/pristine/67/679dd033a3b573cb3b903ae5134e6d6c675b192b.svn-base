<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html" xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui" xmlns:ui="http://java.sun.com/jsf/facelets">
<ui:composition template="/resources/templates/common/layout_dialog.xhtml">
	<ui:define name="content_center">
		<h:form enctype="multipart/form-data">
			<p:panelGrid  styleClass="ui-noborder" columns="2" cellpadding="5">
				<p:outputLabel for="agentName" value="#{labels.agentName}" />
				<p:outputLabel id="agentName" value="#{sysAdminAgentPaymentBean.agentName}" />
				<p:outputLabel for="sysPaymentId" value="#{labels.sysPaymentId}" />
				<p:outputLabel id="sysPaymentId" value="#{sysAdminAgentPaymentBean.sysPaymentId}" />
				<p:outputLabel for="paymentDate" value="#{labels.paymentDate}" />
				<p:outputLabel id="paymentDate" value="#{sysAdminAgentPaymentBean.paymentDateStr}" />
				<p:outputLabel for="paymentValue" value="#{labels.paymentValue}" />
				<p:outputLabel id="paymentValue" value="#{sysAdminAgentPaymentBean.paymentValue}" />

				<p:outputLabel for="currency" value="#{labels.currency}" />
				<p:outputLabel id="currency" value="#{sysAdminAgentPaymentBean.currencyName}" />
				<p:outputLabel for="insertDate" value="#{labels.insertDate}" />
				<p:outputLabel id="insertDate" value="#{sysAdminAgentPaymentBean.insertDateStr}" pattern="#{entityConstantsBean.dateFormat}" />
				<p:outputLabel />
				<p:commandButton value="#{labels.cancel}" actionListener="#{sysAdminAgentPaymentBean.cancelPayment()}"
					rendered="#{sysAdminAgentPaymentBean.statusId!=entityConstantsBean.status.blockStatusId}" />
			</p:panelGrid>
		</h:form>
	</ui:define>
</ui:composition>
</html>