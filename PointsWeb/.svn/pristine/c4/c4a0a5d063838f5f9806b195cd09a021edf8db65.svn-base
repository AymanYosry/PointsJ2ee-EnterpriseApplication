<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html" xmlns:p="http://primefaces.org/ui">
<h:head>
	<link rel="stylesheet" type="text/css" media="print" href="print.css" />
</h:head>

<ui:composition template="/resources/templates/common/layout_dialog.xhtml">
	<ui:define name="content_center">
		<h:form>
			<p:panelGrid styleClass="ui-noborder" id="exchangeData" columns="2">
				<p:outputLabel value="#{labels.receiptNumber}" />
				<p:outputLabel value="#{agentSellerExchangeBean.pointsExchangeId}" />
				<p:outputLabel value="#{labels.userFullName}" />
				<p:outputLabel value="#{agentSellerExchangeBean.profileFullName}" />
				<p:outputLabel value="#{labels.agent}" />
				<p:outputLabel value="#{agentSellerExchangeBean.tradeMark}" />
				<p:outputLabel value="#{labels.branch}" />
				<p:outputLabel value="#{agentSellerExchangeBean.branchName}" />
				<p:outputLabel value="#{labels.productName}" />
				<p:outputLabel value="#{agentSellerExchangeBean.productName}" />
				<p:outputLabel value="#{labels.pointsValue}" />
				<p:outputLabel value="#{agentSellerExchangeBean.pointsValue}" />
			</p:panelGrid>

			<p:commandButton value="${labels.print}" type="button" icon="ui-icon-print" style="display:block;margin-bottom: 20px">
				<p:printer target="exchangeData" />
			</p:commandButton>
		</h:form>
	</ui:define>
</ui:composition>
</html>