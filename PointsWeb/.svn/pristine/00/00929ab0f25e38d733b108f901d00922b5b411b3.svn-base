function validateData()
{
	var formID = 'rf';
	var mobileID  = formID + ':user';
	var confirmMobileID  = formID + ':confirmmobile';
	
	var mobile = document.getElementById(mobileID).value;
	var confirmMobile = document.getElementById(confirmMobileID).value;
	if(mobile != confirmMobile) return false;
}