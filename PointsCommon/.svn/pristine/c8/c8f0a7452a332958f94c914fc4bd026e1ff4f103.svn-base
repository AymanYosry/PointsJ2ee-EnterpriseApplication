package com.ewhale.points.common.exception;

import com.ewhale.points.common.util.ExceptionConstants;

/**
 * @author Ayman Yosry
 */
public class AuthenticationSecurityException extends ApplicationException
{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * 
	 */
	public AuthenticationSecurityException()
	{
		super(ExceptionConstants.SECURITY_EXCETION_STATUS_CODE);
	}

	/**
	 * @param message
	 */
	public AuthenticationSecurityException(String message)
	{
		super(message, ExceptionConstants.SECURITY_EXCETION_STATUS_CODE);
	}

	/**
	 * @param cause
	 */
	public AuthenticationSecurityException(Throwable cause)
	{
		super(cause, ExceptionConstants.SECURITY_EXCETION_STATUS_CODE);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public AuthenticationSecurityException(String message, Throwable cause)
	{
		super(message, cause, ExceptionConstants.SECURITY_EXCETION_STATUS_CODE);
	}
}
